{
    "manifest_version": 2,
    "name":"YoutubeTimer",
    "version":"1.0",
    "description":"Monitor the time wasted on Youtube videos",
        
    "browser_action":{
        "default_popup":"popup.html"
    },

    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
      },

    "permissions":[
        "tabs",
        "storage",
        "activeTab",
        "background"
        
    ],
    "background":{
        "scripts":["background.js"],
        "persistent": false
    }
}
